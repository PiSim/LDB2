<Application x:Class="LabDB2.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:resd="clr-namespace:Infrastructure.Resources;assembly=Infrastructure"
             xmlns:local="clr-namespace:LabDB2">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Infrastructure;component/Resources/CommonResourceDictionary.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Controls;component/Resources/ComboItemTemplatesDictionary.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Reports;component/Resources/ResultPresentationGridDictionary.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Controls;component/Resources/StyleDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
